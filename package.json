{
  "name": "uniswap-v2-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/hanyunx/uniswap-v2-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "create-local": "graph create --node http://localhost:8020/ hanyunx/equalizer-subgraph-fork",
    "deploy-local": "graph deploy hanyunx/equalizer-subgraph-fork --debug --node http://127.0.0.1:8020",
    "deploy": "graph deploy hanyunx/equalizer-subgraph-fork --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "watch-local": "graph deploy hanyunx/equalizer-subgraph-fork --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
